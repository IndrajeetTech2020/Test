    <div class="footer-copy">
        <p>All right reservesd ©CP Jewellery </p>
    </div>
    <!-- //footer -->
    <script>
    function login() {
        debugger
        $.ajax({
            type: 'GET',
            url: '/admin/Login',
            datatype: JSON,
            data: { 'username': $('#username').val(), 'password': $('#password').val() },
            success: function (data) {
                debugger;
                if (data == true) {
                    window.location.href = '/admin/Dashboard';
                }
                else {
                    $('#amessage').css("display", "block");
                }
            },
            error: function (data) { }
        });
    }
    </script>
